<section>   
   <button onclick="misProfesores()">Mis profesores</button>
   <button onclick="misEstudiantes()">Mis estudiantes</button>


   <div id="profesores">
   <h1>Profesores</h1>
   {{#each user}}
   {{!--  {{#each myTeacherState}} --}}
      <div>     
         <p>Profesor de: {{this.teacher.category}}</p>
         <p>{{this.teacher.name}}</p>
         <img src="{{this.teacher.imageProfile}}" alt="">  
         <p>{{this.teacher.description}}</p>
         <p>{{this.state}}</p>
      </div>
    {{!-- {{/each}} --}}
   {{/each}} 
   </div>

   <div id="estudiantes">
      <h1>Estudiantes</h1>
   {{#each myStudents}}
    {{!-- {{#each myStudentState}} --}}
	<p>El estudiante: {{this.student.name}} te ha solicitado.</p> 
      <div>     
         <p>Mi estudiante de: {{this.student.category}}</p>
         <p>{{this.student.name}}</p>
         <img src="{{this.student.imageProfile}}" alt="">  
         <p>{{this.student.description}}</p>
         <p>{{this.state}}</p>
	      <a class="button button--small" href="/profile/message/{{this.student._id}}/accept">Aceptar</a>
         <a class="button button--delete button--small" href="/profile/message/{{this.student._id}}/decline">Rechazar</a>
      </div>
    {{!-- {{/each}} --}}
   {{/each}} 
   </div>
</section>

<script src="/javascripts/matches.js"></script>
